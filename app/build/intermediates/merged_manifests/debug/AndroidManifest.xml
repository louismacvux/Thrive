        android:icon="@mipmap/ic_launcher"
        android:label="@string/Thrive"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Comp3350" >
        <activity android:name="com.comp3350.UI.MainActivity" />
        <activity android:name="com.comp3350.UI.RegisterActivity" />
        <activity android:name="com.comp3350.UI.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.comp3350.UI.WorkoutSuggestion" />
        <activity android:name="com.comp3350.UI.ViewPDF" />
        <activity android:name="com.comp3350.UI.WellnessResult" />
        <activity android:name="com.comp3350.UI.WellnessCalculator" />
        <activity android:name="com.comp3350.UI.BloodInformation" />
    </application>

</manifest>